{
  "name": "company-form-test",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "dev-company-build": "ng build --app company --output-path dist/dev/company",
    "prod-build": "npm run prod-build-en & npm run prod-build-fr",
    "prod-build-en": "ng build -prod --output-path dist/en/ --env=prod-en",
    "prod-build-fr": "ng build -prod --output-path dist/fr/ --env=prod-fr",
    "prod-build-allForms-en": "npm run prod-company-build-external-en & npm run prod-company-build-internal-en & npm run prod-dossier-build-external-en & npm run prod-dossier-build-internal-en & npm run prod-transaction-build-en & npm run prod-applicationInfo-build-en",
    "prod-company-build": "npm run prod-company-build-external & npm run prod-company-build-internal",
    "prod-company-build-external": "npm run prod-company-build-external-en & npm run prod-company-build-external-fr",
    "prod-company-build-external-en": "ng build -prod --output-path dist/prod/company/external/en/ --env=prod-en --app company",
    "prod-company-build-external-fr": "ng build -prod --output-path dist/prod/company/external/fr/ --env=prod-fr --app company",
    "prod-company-build-internal": "npm run prod-company-build-internal-en & npm run prod-company-build-internal-fr",
    "prod-company-build-internal-en": "ng build -prod --output-path dist/prod/company/internal/en/ --env=prod-en --app company_internal",
    "prod-company-build-internal-fr": "ng build -prod --output-path dist/prod/company/internal/fr/ --env=prod-fr --app company_internal",
    "prod-dossier-build": "npm run prod-dossier-build-external & npm run prod-dossier-build-internal",
    "prod-dossier-build-external": "npm run prod-dossier-build-external-en & npm run prod-dossier-build-external-fr",
    "prod-dossier-build-external-en": "ng build -prod --output-path dist/prod/dossier/external/en/ --env=prod-en --app dossier",
    "prod-dossier-build-external-fr": "ng build -prod --output-path dist/prod/dossier/external/fr/ --env=prod-fr --app dossier",
    "prod-dossier-build-internal": "npm run prod-dossier-build-internal-en & npm run prod-dossier-build-internal-fr",
    "prod-dossier-build-internal-en": "ng build -prod --output-path dist/prod/dossier/internal/en/ --env=prod-en --app dossier_internal",
    "prod-dossier-build-internal-fr": "ng build -prod --output-path dist/prod/dossier/internal/fr/ --env=prod-fr --app dossier_internal",
    "prod-transaction-build": "npm run prod-transaction-build-en & npm run prod-transaction-build-fr",
    "prod-transaction-build-en": "ng build -prod --output-path dist/prod/transaction/en/ --env=prod-en --app transaction",
    "prod-transaction-build-fr": "ng build -prod --output-path dist/prod/transaction/fr/ --env=prod-fr --app transaction",
    "prod-applicationInfo-build": "npm run prod-applicationInfo-build-en & npm run prod-applicationInfo-build-fr",
    "prod-applicationInfo-build-en": "ng build -prod --output-path dist/prod/applicationInfo/en/ --env=prod-en --app applicationInfo",
    "prod-applicationInfo-build-fr": "ng build -prod --output-path dist/prod/applicationInfo/fr/ --env=prod-fr --app applicationInfo",
    "test": "ng test",
    "lint": "ng lint  ",
    "e2e": "ng e2e",
    "ng xi18n": "ng xi18n",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.json",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n companyFormTest -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "serve app1": "ng serve --app app1",
    "serve company": "ng serve --app company",
    "serve company-internal": "ng serve --app company_internal",
    "serve dossier": "ng serve --app dossier",
    "serve dossier-internal": "ng serve --app dossier_internal",
    "serve transaction": "ng serve --app transaction",
    "serve applicationInfo": "ng serve --app applicationInfo"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.1.1",
    "@angular/cli": "1.5.5",
    "@angular/common": "5.1.1",
    "@angular/compiler": "5.1.1",
    "@angular/core": "^5.1.1",
    "@angular/forms": "5.1.1",
    "@angular/http": "5.1.1",
    "@angular/platform-browser": "5.1.1",
    "@angular/platform-browser-dynamic": "5.1.1",
    "@angular/router": "5.1.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "@types/file-saver": "^1.3.0",
    "angular2-focus": "^1.1.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.3",
    "file-saver": "^1.3.3",
    "ng2-select": "^2.0.0",
    "rxjs": "^5.5.6",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.5.4",
    "@angular/cli": "1.5.5",
    "@angular/compiler-cli": "5.1.1",
    "@angular/language-service": "5.1.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash": "^4.14.97",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.1.0",
    "compodoc": "0.0.41",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.4",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "xsd4js": "^1.0.2"
  }
}
